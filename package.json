{
  "name": "creativity-tracker",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "compile": "graphql-codegen",
    "watch": "graphql-codegen -w",
    "codegen": "graphql-codegen --config codegen.ts",
    "start:server": "cd server && docker-compose up -d",
    "new:db": "cd server && rm -rf postgres-data && docker-compose build && docker-compose up",
    "it:db": "docker exec -it pg sh",
    "it:cache": "docker exec -it redis redis-cli"
  },
  "dependencies": {
    "@apollo/client": "^3.7.3",
    "@apollo/server": "^4.3.0",
    "@as-integrations/next": "^1.2.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@google-cloud/language": "^5.2.1",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.4",
    "@next/font": "13.1.1",
    "@nivo/core": "^0.80.0",
    "@nivo/pie": "^0.80.0",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "animate.css": "^4.1.1",
    "argon2": "^0.30.3",
    "class-validator": "^0.14.0",
    "eslint": "8.31.0",
    "eslint-config-next": "13.1.1",
    "graphql": "^15.7.2",
    "ioredis": "^5.3.1",
    "next": "13.1.1",
    "next-auth": "^4.19.2",
    "pg": "^8.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-timeclock": "^0.1.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "typescript": "4.9.4",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.16.4",
    "@graphql-codegen/client-preset": "1.2.6",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.2",
    "ts-node": "^10.9.1"
  }
}
